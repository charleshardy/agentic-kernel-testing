{
  "name": "agentic-testing-dashboard",
  "version": "1.0.0",
  "description": "Web dashboard for Agentic AI Testing System",
  "private": true,
  "scripts": {
    "dev": "export XDG_RUNTIME_DIR=${XDG_RUNTIME_DIR:-/tmp/runtime-$USER} && export QT_QPA_PLATFORM=offscreen && export QT_LOGGING_RULES='*=false' && mkdir -p \"$XDG_RUNTIME_DIR\" && vite",
    "dev:clean": "vite",
    "dev:headless": "export DISPLAY= && export XDG_RUNTIME_DIR=/tmp/runtime-$USER && export QT_QPA_PLATFORM=offscreen && export QT_LOGGING_RULES='*=false' && mkdir -p \"$XDG_RUNTIME_DIR\" && vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test-env": "node test-env.js",
    "diagnose": "node diagnose.js"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-window": "^1.8.8",
    "antd": "^5.2.0",
    "axios": "^1.3.0",
    "dayjs": "^1.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.8.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-window": "^2.2.3",
    "recharts": "^2.5.0",
    "socket.io-client": "^4.6.0",
    "zustand": "^4.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-check": "^3.23.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.0",
    "vite": "^4.1.0"
  }
}
