# Staging Environment Configuration

# General Settings
DEBUG=false
LOG_LEVEL=INFO

# LLM Configuration
LLM__PROVIDER=openai
LLM__MODEL=gpt-4
LLM__TEMPERATURE=0.7
LLM__MAX_TOKENS=2000

# Database Configuration
DATABASE__TYPE=postgresql
DATABASE__HOST=postgres-staging.example.com
DATABASE__PORT=5432
DATABASE__NAME=agentic_testing_staging
DATABASE__USER=postgres

# Execution Configuration
EXECUTION__MAX_PARALLEL_TESTS=5
EXECUTION__TEST_TIMEOUT=300
EXECUTION__VIRTUAL_ENV_ENABLED=true
EXECUTION__PHYSICAL_HW_ENABLED=false

# Coverage Configuration
COVERAGE__ENABLED=true
COVERAGE__MIN_LINE_COVERAGE=0.6
COVERAGE__MIN_BRANCH_COVERAGE=0.5

# Security Configuration
SECURITY__FUZZING_ENABLED=true
SECURITY__STATIC_ANALYSIS_ENABLED=true
SECURITY__MAX_FUZZ_TIME=1800

# Performance Configuration
PERFORMANCE__ENABLED=true
PERFORMANCE__REGRESSION_THRESHOLD=0.15
PERFORMANCE__BENCHMARK_ITERATIONS=3

# Notification Configuration
NOTIFICATION__ENABLED=true
NOTIFICATION__EMAIL_ENABLED=false
NOTIFICATION__SLACK_ENABLED=true

# API Configuration
API__DEBUG=false
API__DOCS_ENABLED=true
API__RATE_LIMIT_ENABLED=true
API__RATE_LIMIT_REQUESTS=500